{
  "name": "properties-service",
  "version": "2.0.0",
  "private": true,
  "description": "Real Estate Properties Management Microservice",
  "author": "Real Estate Development Team",
  "keywords": [
    "real-estate",
    "properties",
    "microservice",
    "fastify",
    "prisma"
  ],
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c",
  "scripts": {
    "dev": "tsc && node dist/server.js",
    "dev:watch": "tsc --watch & nodemon dist/server.js",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run src/test/unit",
    "test:integration": "vitest run src/test/integration",
    "test:performance": "vitest run src/test/performance",
    "test:all": "vitest run src/test",
    "test:setup": "powershell -ExecutionPolicy Bypass -File scripts/setup-test-db.ps1",
    "test:setup:bash": "bash scripts/setup-test-db.sh",
    "test:db:migrate": "NODE_ENV=test prisma migrate dev",
    "test:db:generate": "NODE_ENV=test prisma generate",
    "test:db:reset": "NODE_ENV=test prisma migrate reset --force",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset --force",
    "db:seed": "tsx prisma/seed.ts",
    "docker:build": "docker build -t properties-service .",
    "docker:run": "docker run -p 8082:8082 properties-service",
    "clean": "rm -rf dist",
    "prebuild": "yarn clean",
    "prestart": "yarn build"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/request-context": "^5.1.0",
    "@fastify/static": "^7.0.4",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@fastify/under-pressure": "^8.3.0",
    "@prisma/client": "^6.14.0",
    "dotenv": "^17.2.1",
    "fastify": "^4.28.1",
    "fastify-type-provider-zod": "^1.1.9",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "sharp": "^0.33.5",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.3.3",
    "prisma": "^6.14.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0",
    "yarn": ">=4.0.0"
  },
  "main": "dist/server.js",
  "files": [
    "dist/",
    "prisma/",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/properties-service.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-org/properties-service/issues"
  },
  "homepage": "https://github.com/your-org/properties-service#readme"
}
